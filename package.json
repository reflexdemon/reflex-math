{
  "name": "reflex-math",
  "version": "1.0.0",
  "description": "Reflex Math API",
  "main": "index.js",
  "scripts": {
    "start": "defaultLevel=INFO PORT=$PORT node dist/index.js",
    "compile": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "build": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "LOG_LEVEL=info APP_ID=reflex-math nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "dev:debug": "LOG_LEVEL=info APP_ID=reflex-math nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test": "mocha --require @babel/register --exit",
    "test-with-coverage": "nyc --reporter=text mocha --require @babel/register --exit",
    "test:debug": "mocha --require @babel/register --inspect-brk --exit",
    "lint": "eslint -c .eslintrc.json {server,test}/**",
    "lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-openapi-validator": "^1.4.0",
    "handlebars": "^4.1.2",
    "middleware-stack-printer": "0.0.1",
    "pino": "^5.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "author": "Venkateswara VP <contact@vpv.io> (https://github.com/reflexdemon)"
}
